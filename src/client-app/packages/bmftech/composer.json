{
  "name": "Rubel/bmftech",
  "description": "The theme package for bmf-tech.com.",
  "type": "project",
  "license": "MIT",
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "6.*",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "BmfTech\\": "src/"
    },
    "classmap": ["../../../core-app/app/"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "require": {
    "doctrine/dbal": "^2.8",
    "nesbot/carbon": "^1.32"
  }
}